

@{
    Layout = null;
}
<div class="layout_padding gallery_section">
    <div class="container">
        <div class="row">
            @foreach (var item in ViewBag.items)
            {
                <div class="col-sm-4" @*onclick="getItems(@subCate.SubCategorieId+'')*@>
                    <div class="best_shoes">

                        <p class="best_text">@item.name</p>
                        <div class="shoes_icon"><img src="~/Content/Template/images/shoes-img4.png"></div>
                        <div class="star_text">
                            <div class="left_part">
                                <ul>
                                    <li><a href="#"><img src="/Content/Template/images/star-icon.png"></a></li>
                                    <li><a href="#"><img src="/Content/Template/images/star-icon.png"></a></li>
                                    <li><a href="#"><img src="/Content/Template/images/star-icon.png"></a></li>
                                    <li><a href="#"><img src="/Content/Template/images/star-icon.png"></a></li>
                                    <li><a href="#"><img src="/Content/Template/images/star-icon.png"></a></li>
                                </ul>
                            </div>
                            <div class="right_part">
                                <div class="shoes_price">Price : <span style="color: #ff4e5b;">@item.Price</span></div>
                            </div>
                        </div>
                    </div>
                </div>
            }

        </div>
      
    </div>
</div>
